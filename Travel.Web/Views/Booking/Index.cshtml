@model Travel.Web.ViewModels.BookingViewModel

@{
    ViewBag.Title = "login";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Login</h2>

<form>
    <div class="form-group">
        <label for="BookingReference">Booking reference</label>
        <input type="text" value="" class="form-control" id="BookingReference" name="BookingReference">
    </div>
    <div class="form-group">
        <label for="LastName">Last name</label>
        <input type="text" class="form-control" id="LastName" name="LastName">
    </div>
    <button id="loginUser" type="submit" class="btn btn-primary">Login</button>
</form>

@section{
    <script src="~/Scripts/App/booking.js" type="text/javascript"></script>
}